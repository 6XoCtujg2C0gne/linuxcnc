/****** VERTICAL SLIDER ************/
QSlider::groove:vertical {
  background: #26261e;
  border-radius: 4px;
  width: 20px }

QSlider::handle:vertical {
  background: #ffee06;
  border: 0px solid #ffee06;
  border-radius: 4px;
  height: 24px }

QSlider::add-page:vertical {
  background: #ffee06;
  border: 1px solid #ffee06;
  border-radius: 4px;
  width: 20px }

QSlider::sub-page:vertical {
  background: #26261e;
  border: 1px solid #26261e;
  border-radius: 4px;
  width: 16px }

QSlider::add-page:vertical:disabled,
QSlider::sub-page:vertical:disabled,
QSlider::handle:vertical:disabled,
QSlider::groove:vertical:disabled {
  color: #b0b0b0;
  background: #26261e;
  border: 1px solid #b0b0b0;
  border-radius: 4px }

/****** FILE MANAGER & MDI HISTORY************/
#filemanager QListView,
#mdihistory QListView {
  border: 1px solid #ffee06;
  border-radius: 4px;
  font: 14pt 'Lato';
  alternate-background-color: #26261e;
  show-decoration-selected: 1;
  selection-background-color: red;
  selection-color:cyan;
  padding: 2 }

#filemanager QListView::item,
#mdihistory QListView::item {
  height: 36;
}

#filemanager QListView::item:selected:active,
#mdihistory QListView::item:selected:active {
  color: #16160e;
  background-color: #ffee06 }

#filemanager QListView::item:selected:!active,
#mdihistory QListView::item:selected:!active {
  color: #16160e;
  background-color: #ffee06 }

#filemanager QPushButton {
  font: 10pt;
  height: 40;
  width: 60;
}

#filemanager QComboBox {
  height: 40;
  font: 12pt;
alternate-background-color: red;
 }

#mdihistory QLineEdit {
  border: 1px solid #ffee06;
  border-radius: 4px;
  height: 30px;
  font: 14pt }

/****** COMBO BOX ************/
QComboBox {
  color: #ffee06;
  background: #16160e;
  border: 1px solid #ffee06;
  border-radius: 4px;
  font: 11pt 'Lato';
  padding: 0px 0px 0px 16px; }

QComboBox::drop-down {
  image: none; }

QComboBox QListView,
QComboBox#filemanager QListView {
  border: 8px solid #ffee06;
  border-radius: 0px; }

