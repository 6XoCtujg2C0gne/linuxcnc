/****** SCROLL BAR ************/
QScrollBar:horizontal {
  background: #26261e;
  border: 0px;
  border-radius: 4px;
  margin: 0px;
  height: 20px }

QScrollBar::handle:horizontal {
/*  background: #ffee06;*/
  background: #ffee06;
  border: 2px solid #ffee06;
  border-radius: 4px;
  margin: 2px;
  min-width: 40px }

QScrollBar::add-line:horizontal {
  width: 0px }

QScrollBar::sub-line:horizontal {
  width: 0px }

QScrollBar:vertical {
  background: #26261e;
  border: 0px;
  border-radius: 4px;
  margin: 0px;
  width: 20px }

QScrollBar::handle:vertical {
/*  background: #ffee06;*/
  background: #ffee06;
  border: 2px solid #ffee06;
  border-radius: 4px;
  margin: 2px;
  min-height: 40px }

QScrollBar::add-line:vertical {
  height: 0px }

QScrollBar::sub-line:vertical {
  height: 0px }

/****** MENU ************/
QMenu {
  background: #ffee06;
  font: 14pt;
  margin: 4px }

QMenu::item {
  color: #ffee06;
  background: #16160e;
  border: 4px solid #ffee06;
  padding: 8px }

/****** TAB ************/
QTabWidget::pane {
  border: none;
  top:-2px; 
  left:-2px; 
  right:-2px; 
  bottom:-2px }

QTabWidget::tab-bar {
  min-width:6000 }

QTabBar::tab {
  border: 1px  solid #ffee06;
  border-radius: 4px;
  padding: 1px;
  margin-left: 1;
  margin-right: 1 } 

QTabBar::tab:selected {
  color: #16160e;
  background: #ffee06 }

/****** STATUS BAR ************/
QStatusBar {
border: none }

QStatusBar::item {
 image: none }

QProgressBar {
  color: #ffee06;
  border: 1px solid #ffee06;
  border-radius: 4px }

QProgressBar::chunk {
  background-color: #ffee06 }

/****** LED ************/
LED {
  qproperty-color: #ffee06;
  qproperty-diameter: 20 }

/****** TEXT ENTRY ************/
QPlainTextEdit {
  border: 1px solid #ffee06;
  border-radius: 4px }

/****** GCODE EDITOR ************/
#gcode_editorTopButtonFrame {
  border: 1px solid #ffee06;
  border-radius: 4px;
  margin: 2 4 0 4 }

#gcode_editorBottomButtonFrame {
  border: 1px solid #ffee06;
  border-radius: 4px;
  margin: 0 4 0 4 }

#gcode_editor QToolBar {
  spacing: 4 }

#gcode_editor QToolButton {
  width: 40px;
  height: 40px }

#gcode_editor QLineEdit {
  border: 1px solid #ffee06;
  border-radius: 4px;
  height: 40px }

/****** MESSAGE BOX ************/
QMessageBox{
  border: 1px solid #ffee06;
  border-radius: 4px;
  margin: 2 }

QMessageBox QLabel{
  font: 12pt }

QMessageBox QPushButton {
  width: 120;
  height: 40;
  font: 12pt }

/****** INPUT DIALOG ************/
QInputDialog{
  border: 1px solid #ffee06;
  border-radius: 4px;
  margin: 2 }

QInputDialog QLabel{
  font: 16pt }

QInputDialog QLineEdit{
  background: #26261e;
  border: 1px solid #ffee06;
  border-radius: 4px;
  font: 16pt }

QInputDialog QPushButton {
  height: 40;
  font: 16pt }

/****** CALCULATOR ************/
Calculator {
  border: 1px solid #ffee06;
  border-radius: 4px;
  margin: 2px }

Calculator QPushButton {
  height: 40 }

Calculator QLineEdit {
  border: 1px solid #ffee06;
  border-radius: 4px;
  font: 20pt }

/****** SOFT KEYBOARD ************/
SoftInputWidget {
border: 1px solid #ffee06;
border-radius: 4px }

MyFlatPushButton {
margin:3px }






